    "@xterm/addon-unicode11": "^0.9.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/addon-webgl": "^0.19.0",
    "@xterm/xterm": "^6.0.0",
    "solid-js": "^1.8",
    "xterm": "^5.3",
    "xterm-addon-fit": "^0.8",
    "xterm-addon-webgl": "^0.16"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "autoprefixer": "^10",
    "postcss": "^8",
