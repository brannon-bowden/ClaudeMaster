{
  "name": "claude-master",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "npm run sidecar:build && tauri dev",
    "tauri:build": "npm run sidecar:build && tauri build",
    "tauri:build:debug": "npm run sidecar:build && tauri build --debug",
